{
  "name": "humhub/humhub-calendar",
  "description": "HumHub calendar module",
  "type": "library",
  "require": {
    "yiisoft/yii2": "dev-master",
    "sabre/vobject": "^4",
    "simshaun/recurr": "^4",
    "sabre/xml": "^2",
    "sabre/uri": "^2",
    "sabre/dav": "dev-master"
    "sabre/dav": "~4.6.0",
  },
  "config": {
    "process-timeout": 1800,
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "minimum-stability": "dev"
}
